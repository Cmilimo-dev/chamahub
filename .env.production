# Production Environment Configuration
VITE_ENVIRONMENT=production

# Supabase Production Configuration (Replace with your production instance)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# M-Pesa Production Configuration
VITE_MPESA_ENVIRONMENT=production
VITE_MPESA_CONSUMER_KEY=your-production-consumer-key
VITE_MPESA_CONSUMER_SECRET=your-production-consumer-secret
VITE_MPESA_SHORTCODE=your-production-shortcode
VITE_MPESA_PASSKEY=your-production-passkey

# Push Notifications
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key
VITE_VAPID_PRIVATE_KEY=your-vapid-private-key

# App Configuration
VITE_APP_URL=https://yourdomain.com
VITE_APP_NAME=ChamaHub
VITE_APP_VERSION=1.0.0

# Analytics and Monitoring
VITE_ENABLE_ANALYTICS=true
VITE_SENTRY_DSN=your-sentry-dsn
VITE_GOOGLE_ANALYTICS_ID=your-ga-id

# Performance
VITE_SUPPRESS_CLOUDFLARE_WARNINGS=false
VITE_DEBUG_REALTIME=false
